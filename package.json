		"elysia": "latest",
		"flagsmith-nodejs": "^5.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@elysiajs/eden": "^1.2.0",
		"@expo/server": "^0.5.3",
		"bun-types": "latest"
	},
	"module": "src/index.js"
